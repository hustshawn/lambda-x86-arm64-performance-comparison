{
  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        ".kiro/settings/.env",
        "ghcr.io/github/github-mcp-server"
      ],
      "disabled": false,
      "autoApprove": [
        "get_me",
        "list_workflows",
        "list_workflow_runs",
        "list_workflow_jobs",
        "get_job_logs",
        "search_repositories",
        "get_workflow_run"
      ]
    }
  }
}